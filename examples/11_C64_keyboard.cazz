'keyboard_loc 197
'quit false
print "PRESS KEY - Q TO QUIT"
'i 0
while quit ! [
    'key peek keyboard_loc
    key: [key + 64] !<
    quit: key = 126 or i > 10
    if [key = 128] ! [
        prin "YOU PRESSED: "
        emit key
        prin " CODE: "
        prin key
        print " !"
    ]
    inc i
]