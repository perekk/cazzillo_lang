'j 0 
while j < 12 [
    'i 0 !< 
    while i < 25 [
        'x [i + j] !< 
        'y i 
        [
            'cursor 1024 + y * 40 + x 
            'cursorlo cursor !< 
            'cursorhi [cursor / 256] !< 
            asm [
                "; GOTOXY" 
                "LDA !x" 
                "STA CURX" 
                "LDA !y" 
                "STA CURY" 
                "LDA !cursorlo" 
                "STA CURSOR" 
                "LDA !cursorhi" 
                "STA CURSOR+1"
            ]
        ] 
        prin "CIAO" 
        inc i
    ] 
    j: j + 4
]

prin "END"